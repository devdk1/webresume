<% include header.ejs %>

  <body id="top">
    <div id="intro_about">
      <section id="intro">
        <div class="intro-overlay"></div>
        <div class="intro-content">
          <div class="row">
            <div class="col-twelve">
              <h5>
                <%= intro_text_1 %>
              </h5>
              <h1>
                <%= intro_text_2 %>
              </h1>
              <p class="intro-position">
                <% for(var i = 0; i < designations.length; i++) { %>
                  <span><%= designations[i] %></span>
                  <% } %>
              </p>
            </div>
          </div>
        </div>

        <ul class="intro-social">
          <% for(var i = 0; i < social_tabs.length; i++) { %>
            <li><a href="<%= social_tabs[i].link %>" target=_blank><i class="<%= social_tabs[i].class_name %>"></i></a></li>
            <% } %>
        </ul>
      </section>

      <section id="about">
        <div class="row section-intro">
          <div class="col-twelve">
            <div class="intro-info">
              <p class="lead"> <%= about_paragraph %> </p>
              <!-- <a href="#experience" class="button stroke smoothscroll" data-id="experience" data-value="intro_about" title="">My Work Experience</a>   -->
            </div>
          </div>
        </div>
      </section>
    </div>
    <!-- /process-->


    <!-- resume Section
    ================================================== -->
    <section id="experience" class="grey-section">
      <div class="row resume-timeline">
        <div class="col-twelve resume-header">
          <h2>Work Experience</h2>
        </div>
        <div class="col-twelve">
          <% info_exp.forEach((info) => { %>
            <div class="timeline-wrap pull-left">
              <div class="col-twelve">
                <div class="row">      
                  <div class="col-six pull-left">
                    <strong class='h1-font'><%= info.name.toUpperCase() %></strong>
                    <p class='font-italic'><%= info.location %>, <%= info.range %></p>
                  </div>
                  <div class="col-six pull-right">
                    <div class="col-six pull-right">
                      <a href="<%= info.link %>" target="_blank"><img class="pull-right max-height-width-100" src="<%= info.img_src %>" data-toggle="tooltip" title="<%= info.name.toUpperCase() %>"></a>
                    </div>
                  </div>
                </div>
              </div>
              <% info.about_post.forEach((about_p) => { %>
                <div class="col-twelve padding-btm-3">
                  <h3 style="margin-top:5rem;"></h3><%= about_p.post_head.toUpperCase()%> ( <%= about_p.range.toUpperCase() %> )</h3>
                  <ul class="box">
                    <% var i = 0; %>
                    <% about_p.post_desc.forEach((post_d) => { %>
                      <li><i class="fa fa fa-angle-double-right fa-fw" aria-hidden="true"></i><%= post_d %> <% if (i == 1) { %> <a href="https://apps.shopify.com/multi-vendor-marketplace" target="_blank">App Link</a> <% } %> </li>
                      <% i = i + 1; %>
                    <% }) %>
                  </ul>
                </div>
              <% }) %>
            </div>
          <% }) %>
        </div>
        <div class="row">
          <div class="col-twelve text-center">
            <!-- <a href='#education' class="button stroke smoothscroll" data-id="education" data-value="experience" title="">My Education</a> -->
          </div>
        </div>
      </div>
    </section>
    <section id="education" class="grey-section">   
        <div class="row resume-timeline">
          <div class="col-twelve resume-header">
            <h2>Education</h2>
          </div>
          <div class="col-twelve">
            <div class="timeline-wrap">
              <% info_edu.forEach((edu) => { %>
                <div class="timeline-block">
                  <div class="timeline-ico">
                    <i class="fa fa-briefcase"></i>
                  </div>
                  <div class="timeline-header">
                    <h3><%= edu.degree_name %></h3>
                    <span>( <%= edu.degree_duration %> )</span>
                  </div>
                  <div class="timeline-content">
                    <h4><%= edu.institue %></h4>
                    <p><%= edu.about_edu %></p>
                  </div>
                </div>
              <% }) %>
            </div>
          </div>
          <div class="row">
              <div class="col-twelve text-center" style="margin-top: 3rem;">
                <!-- <a href='#services' class="button stroke smoothscroll" data-id="education" data-value="experience" title="">skills</a> -->
              </div>
            </div>
        </div>
    </section>

    <section id="services">
      <div class="overlay"></div>
      <div class="row section-intro">
        <div class="col-twelve">
          <h5>Objective</h5>
          <h1></h1>
          <p class="lead">To work in dynamic, professional and enterprising environment which will provide me the potential for advancement and increased IT skills, decision- making responsibilities through consistent learning under the business and IT leaders </p>
